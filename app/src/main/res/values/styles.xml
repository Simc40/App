<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="roundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="background">@color/gray_background</item>
    </style>

    <style name="whiteTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="blackTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="logo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:contentDescription">@string/todo</item>
        <item name="srcCompat">@drawable/logo</item>
    </style>

    <style name="goBack">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/goBack</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="registerInputBox">
        <item name="android:layout_width">370dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="backgroundColor">@color/white</item>
        <item name="cardCornerRadius">15dp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="registerInput">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_margin">20dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="submitForm">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="cardBackgroundColor">@color/greenButton</item>
        <item name="cardCornerRadius">35dp</item>
    </style>

    <style name="submitFormText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="tableGridView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:stretchColumns">0,1</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="tableRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="gridViewLeftCard">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">12dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginStart">30dp</item>
        <item name="cardBackgroundColor">@color/white</item>
        <item name="cardCornerRadius">15dp</item>
        <item name="android:layout_column">0</item>
        <item name="cardElevation">2dp</item>
    </style>

    <style name="gridViewRightCard">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">30dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginStart">12dp</item>
        <item name="cardBackgroundColor">@color/white</item>
        <item name="cardCornerRadius">15dp</item>
        <item name="android:layout_column">0</item>
        <item name="cardElevation">2dp</item>
    </style>

    <style name="cardInnerLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">25dp</item>
    </style>

    <style name="imageFromCard">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
    </style>

    <style name="textFromCard">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">2</item>
        <item name="autoSizeMaxTextSize">15sp</item>
        <item name="autoSizeMinTextSize">8sp</item>
        <item name="autoSizeStepGranularity">1sp</item>
        <item name="autoSizeTextType">uniform</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="LinearLayoutMatchWrapVert">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="layoutMatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="firebaseListScrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="horizontalMargin20dp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>

    <style name="horTopMargin20dp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
    </style>

    <style name="mQualidadeBlackHeaderText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="mQualidadeWhiteViewUnderBlackHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="WhiteCard">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="cardBackgroundColor">@color/white</item>
        <item name="cardCornerRadius">20dp</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="WhiteCardText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">20dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="mQualidadeButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="cardCornerRadius">15dp</item>
    </style>

    <style name="mQualidadeButtonText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:padding">15dp</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="whiteLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="mQualidadeImages">
        <item name="android:layout_width">160dp</item>
        <item name="android:layout_height">160dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="tableLeftColumn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">2</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@color/light_gray</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginStart">2dp</item>
        <item name="android:layout_marginEnd">2dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:padding">18dp</item>
    </style>

    <style name="tableRightColumn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginEnd">2dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:padding">16dp</item>
        <item name="android:inputType">number</item>
    </style>

    <style name="twoColumnTable">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:background">@color/black</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:stretchColumns">0,1</item>
    </style>

    <style name="twoColumnTableRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/black</item>
        <item name="android:layout_marginEnd">2dp</item>
    </style>

    <style name="twoColumnTableLeftColumnLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/light_gray</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginStart">2dp</item>
        <item name="android:layout_marginEnd">2dp</item>
    </style>

    <style name="twoColumnTableLeftColumnTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@color/light_gray</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:padding">18dp</item>
    </style>

    <style name="twoColumnTableRightColumnLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginEnd">2dp</item>
    </style>

    <style name="twoColumnTableRightColumnTextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:scrollHorizontally">false</item>
        <item name="android:ellipsize">none</item>
        <item name="android:maxWidth">150dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:inputType">textMultiLine</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:padding">18dp</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
</resources>