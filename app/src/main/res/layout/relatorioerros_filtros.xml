                        android:paddingBottom="16dp"
                        android:scrollbarStyle="outsideOverlay"
                        app:tsquare_orientation_horizontal="true"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0"
                        app:tsquare_dividerColor="@color/zxing_transparent"
                        app:tsquare_headerTextColor="@color/black" />

                    <androidx.cardview.widget.CardView
                        android:id="@+id/solutionSubmitData"
                        style="@style/mQualidadeButton"
                        app:cardBackgroundColor="@color/black"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/solutionCalendar">
                        <TextView
                            style="@style/mQualidadeButtonText"
                            android:textColor="@color/white"
                            android:text="Inserir Data"/>
                    </androidx.cardview.widget.CardView>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.cardview.widget.CardView
                android:id="@+id/applyFilters"
                style="@style/mQualidadeButton"
                app:cardBackgroundColor="@color/black"
                android:layout_marginBottom="20dp">
                <TextView
                    style="@style/mQualidadeButtonText"
                    android:textColor="@color/white"
                    android:text="Aplicar Filtros"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>