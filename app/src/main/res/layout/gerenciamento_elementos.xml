<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_background"
    tools:context=".moduloGerenciamento.GerenciamentoElementos">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/header"
                android:layout_marginTop="20dp"
                style="@style/horizontalMargin20dp">

                <ImageView
                    android:id="@+id/logo"
                    style="@style/logo"
                    android:contentDescription="@string/todo" />

                <TextView
                    android:id="@+id/goBack"
                    style="@style/goBack"
                    android:layout_marginTop="10dp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/title"
                android:layout_marginTop="10dp"
                style="@style/horizontalMargin20dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/titleLogo"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:padding="10dp"
                    android:contentDescription="@string/todo"
                    android:src="@drawable/wrench" />
                <androidx.cardview.widget.CardView
                    android:id="@+id/titleCard"
                    android:layout_marginTop="15dp"
                    style="@style/horizontalMargin20dp"
                    app:cardBackgroundColor="#000000"
                    app:cardCornerRadius="15dp">
                    <TextView
                        style="@style/mQualidadeButtonText"
                    android:text="@string/moduloGerenciamentoElementosTitle"
                        android:textColor="@color/white"
                        android:textSize="20sp" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <RelativeLayout
                android:layout_marginTop="20dp"
                style="@style/horizontalMargin20dp">
                <androidx.cardview.widget.CardView
                    android:id="@+id/obraHeader"
                    style="@style/layoutMatchWrap"
                    app:cardBackgroundColor="@color/black">
                    <TextView
                        style="@style/mQualidadeBlackHeaderText"
                        android:text="@string/moduloGerenciamentoElementosObra"/>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_below="@+id/obraHeader"
                    style="@style/WhiteCard">
                    <TextView
                        android:id="@+id/nomeObra"
                        style="@style/WhiteCardText" />
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    style="@style/mQualidadeWhiteViewUnderBlackHeader"
                    android:layout_below="@+id/obraHeader"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="20dp"
                style="@style/horizontalMargin20dp">
                <androidx.cardview.widget.CardView
                    android:id="@+id/nomeModeloHeader"
                    style="@style/layoutMatchWrap"
                    app:cardBackgroundColor="@color/black">
                    <TextView
                        style="@style/mQualidadeBlackHeaderText"
                        android:text="@string/moduloGerenciamentoElementosNome"/>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_below="@+id/nomeModeloHeader"
                    style="@style/WhiteCard">
                    <TextView
                        android:id="@+id/nomeElemento"
                        style="@style/WhiteCardText" />
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    style="@style/mQualidadeWhiteViewUnderBlackHeader"
                    android:layout_below="@+id/nomeModeloHeader"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="20dp"
                style="@style/horizontalMargin20dp">
                <androidx.cardview.widget.CardView
                    android:id="@+id/numMaxHeader"
                    style="@style/layoutMatchWrap"
                    app:cardBackgroundColor="@color/black">
                    <TextView
                        style="@style/mQualidadeBlackHeaderText"
                        android:text="@string/moduloGerenciamentoElementosTipo"/>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_below="@+id/numMaxHeader"
                    style="@style/WhiteCard">
                    <TextView
                        android:id="@+id/tipoElemento"
                        style="@style/WhiteCardText" />
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    style="@style/mQualidadeWhiteViewUnderBlackHeader"
                    android:layout_below="@+id/numMaxHeader"/>
            </RelativeLayout>

            <ImageView
                android:id="@+id/imagemTipo"
                style="@style/horTopMargin20dp"
                android:layout_height="180dp"
                android:contentDescription="@string/todo" />

            <View
                style="@style/whiteLine"/>

            <RelativeLayout
                style="@style/horTopMargin20dp">
                <androidx.cardview.widget.CardView
                    style="@style/layoutMatchWrap"
                    android:layout_height="10dp"
                    app:cardBackgroundColor="#000000"
                    android:layout_alignParentBottom="true">

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    style="@style/layoutMatchWrap"
                    app:cardBackgroundColor="#000000"
                    app:cardCornerRadius="15dp">
                    <TextView
                        style="@style/mQualidadeButtonText"
                        android:text="@string/moduloGerenciamentoElementosTableTitle"
                        android:textColor="#FFFFFF" />
                </androidx.cardview.widget.CardView>
            </RelativeLayout>

            <TableLayout
                android:id="@+id/tablenovaforma"
                style="@style/twoColumnTable">

                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosForma"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/forma"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosB"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/b"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosH"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/h"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosC"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/c"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosPecasPlanejadas"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/pecasPlanejadas"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosPecasCadastradas"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/pecasCadastradas"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosFckDesforma"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/fckDesf"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosVolume"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/fckIc"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosPeso"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/volume"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosTaxaAco"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/peso"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/moduloGerenciamentoElementosFckIcamento"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/taxaaco"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/dataCriacao"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/creation"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/autorCriacao"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/createdBy"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/ultimaModificacao"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/lastModifiedOn"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    style="@style/twoColumnTableRow">
                    <LinearLayout
                        style="@style/twoColumnTableLeftColumnLayout">
                        <TextView
                            style="@style/twoColumnTableLeftColumnTextView"
                            android:text="@string/autorModificacao"/>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/twoColumnTableRightColumnLayout">
                        <TextView
                            android:id="@+id/lastModifiedBy"
                            style="@style/twoColumnTableRightColumnTextView"/>
                    </LinearLayout>
                </TableRow>
            </TableLayout>

            <androidx.cardview.widget.CardView
                android:id="@+id/PDF"
                app:cardBackgroundColor="@color/black"
                style="@style/mQualidadeButton">
                <TextView
                    style="@style/mQualidadeButtonText"
                    android:textColor="@color/white"
                    android:text="@string/visualizarPDF"/>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/reportarErro"
                app:cardBackgroundColor="@color/red"
                style="@style/mQualidadeButton">
                <TextView
                    style="@style/mQualidadeButtonText"
                    android:textColor="@color/white"
                    android:text="@string/reportarErroInformacao"/>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/goBackEnd"
                app:cardBackgroundColor="@color/greenButton"
                android:layout_marginBottom="20dp"
                style="@style/mQualidadeButton">
                <TextView
                    style="@style/mQualidadeButtonText"
                    android:textColor="@color/white"
                    android:text="@string/goBack"/>
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>